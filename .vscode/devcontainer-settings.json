{
    "dart.sdkPath": "/sdks/flutter/bin/cache/dart-sdk",
    "dart.flutterSdkPath": "/sdks/flutter",
    "[dart]": {
      "editor.formatOnSave": true,
      "editor.formatOnType": true,
      "editor.rulers": [
        80
      ],
      "editor.tabSize": 2,
      "editor.selectionHighlight": false,
      "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on",
        "other": "on"
      },
      "editor.suggestSelection": "recentlyUsed",
      "editor.tabCompletion": "on",
      "editor.suggestOnTriggerCharacters": true,
      "editor.acceptSuggestionOnEnter": "smart",
      "editor.suggest.showSnippets": false,
      "editor.snippetSuggestions": "none",
      "editor.suggest.showKeywords": false,
      "editor.suggest.insertMode": "replace",
      "editor.autoSurround": "languageDefined",
      "editor.suggest.filterGraceful": true,
      "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.organizeImports": true,
        "addMissingImports": true
      },
      "files.insertFinalNewline": true
    },
    "dart.normalizeFileCasing": true,

    "dart.openDevTools": "flutter",
    "dart.showSkippedTests": true,
    "dart.renameFilesWithClasses": "always",
    "dart.warnWhenEditingFilesInPubCache": true,
    "flutterStylizer": {
      "groupAndSortGetterMethods": false,
      "memberOrdering": [
        "public-instance-variables",
        "private-static-variables",
        "public-constructor",
        "named-constructors",
        "public-static-variables",
        "public-override-variables",
        "private-instance-variables",
        "public-override-methods",
        "public-other-methods",
        "private-other-methods",
        "build-method"
      ],
      "sortOtherMethods": false
    },
    "files.defaultLanguage": "dart",
    "files.exclude": {
      "**.dart_tool": true,
      "**.idea": true,
      "**/*.g.dart": true,
      "**/*.inject.dart": true,
      "**/*.inject.summary": true,
      "**/*.sql": true,
      "**/*.bak": true,
      "**/*.cache": true
    },
    "search.exclude": {
      ".flutter-plugins": true,
      ".flutter-plugins-dependencies": true,
      ".gitignore": true,
      ".metadata": true,
      "**/.fvm": true,
      "**/.vscode": true,
      "**/*.iml": true,
      "**/build": true,
      "debug.log": true,
      "android/**": false,
      "pubspec.lock": true
    }
  }
